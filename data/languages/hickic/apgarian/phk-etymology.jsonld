  "baety:majorSoundChanges": [
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "h-liquid cluster simplification",
      "baety:soundChangeRule": "h [liquid] → h, [liquid] h → h",
      "baety:example": "haru → hau, bahu → bau",
      "baety:notes": "Sequences of h and liquids (l, r) were simplified by dropping the liquid"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Liquid-liquid simplification",
      "baety:soundChangeRule": "[liquid] l → l",
      "baety:example": "harlu → halu",
      "baety:notes": "Clusters of a liquid followed by l were simplified to just l"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Compound vowel deletion",
      "baety:soundChangeRule": "C V → C / _ $$",
      "baety:example": "heru-storu → her-stor",
      "baety:notes": "Vowels at compound boundaries ($$) were deleted"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Compound boundary consonant cluster simplification",
      "baety:soundChangeRule": "s [fricative/glide] → s, θ [stop] → θ / _ $$",
      "baety:example": "kis-θalu → kis-alu",
      "baety:notes": "Certain consonant clusters across compound boundaries were simplified"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Compound nasal/liquid deletion",
      "baety:soundChangeRule": "[nasal/liquid] → ∅ / C V? $$ _, _ $$ C",
      "baety:example": "heru-numa → her-uma",
      "baety:notes": "Nasals and liquids were dropped at compound boundaries in certain environments"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Initial sacred ha- change",
      "baety:soundChangeRule": "ha → θə / $ _ [stop/fricative/liquid/glide]",
      "baety:example": "hataru → θətaru",
      "baety:notes": "Initial ha- was replaced with θə- before certain consonants, marking sacred terms"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Initial glottal devoicing",
      "baety:soundChangeRule": "[voiced glottal] → [unvoiced glottal] / $ _ V",
      "baety:example": "xalu → halu",
      "baety:notes": "Word-initial voiced glottals (x) became unvoiced (h)"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Stress shift to first syllable",
      "baety:soundChangeRule": "[primary] → [unstressed], [unstressed] → [primary] / $ _",
      "baety:example": "heˈru → ˈheru",
      "baety:notes": "The stress pattern changed from penultimate to initial syllable"
    }
  ],
