  "baety:majorSoundChanges": [
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Vowel coalescence",
      "baety:soundChangeRule": "@vowel {h,l} => @vowel * / _ @vowel",
      "baety:example": "ˈmaho → ˈmao, ˈʔali → ˈʔai",
      "baety:notes": "The consonants [h] and [l] were dropped between vowels, causing vowel sequences"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Complex coda simplification",
      "baety:soundChangeRule": "[sonorant] => * / _ [consonant]$",
      "baety:example": "ˈkarn → ˈkar, ˈsint → ˈsit",
      "baety:notes": "Sonorants (nasals, liquids) were dropped before word-final consonants"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Glottal stop dropping",
      "baety:soundChangeRule": "ʔ => * / @vowel _ @vowel",
      "baety:example": "ˈbaʔa → ˈbaa → ˈba",
      "baety:notes": "Glottal stops were dropped between vowels, often resulting in long vowels that later shortened"
    }
  ],
