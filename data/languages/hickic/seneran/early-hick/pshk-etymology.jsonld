  "baety:majorSoundChanges": [
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Initial h-dropping (Low vowels)",
      "baety:soundChangeRule": "h → * / $ _ @vowel&![+high]",
      "baety:example": "ˈhak → ˈak, ˈhau → ˈau",
      "baety:notes": "Word-initial [h] was dropped before non-high vowels"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Animacy prefix reanalysis",
      "baety:soundChangeRule": "hi → ʔi / $ _ {[+high], @consonant&![glottal stop]}",
      "baety:example": "hiˈdag → ʔiˈdag, hiʔal → ʔiʔal",
      "baety:notes": "Initial [hi] became [ʔi] before high vowels or consonants (except glottal stop)"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Post-nasal stop voicing",
      "baety:soundChangeRule": "[stop, -voice] → [+voice] / [nasal] _",
      "baety:example": "ˈmenta → ˈmenda, ˈtunka → ˈtunga",
      "baety:notes": "Voiceless stops became voiced after nasals"
    }
  ],
