      "etymology": {
        "@type": "etymon:Etymology",
        "etymon:protoform": "Proto-Hick *li",
        "etymon:note": "Ancient diminutive marker, preserved in many compounds",
        "etymon:soundChange": "stable form, triggers compound fusion"
      },
      "sense": [
        {
          "@type": "ontolex:LexicalSense",
          "definition": {
            "@value": "diminutive prefix: small, lesser, young",
            "@language": "en"
          },
          "usage": "Highly productive prefix forming diminutives and terms for smaller/lesser versions",
          "lexinfo:semanticField": [
            "Size Terms",
            "Basic Terms",
            "Derivational Morphology"
          ],
          "examples": [
            "limal 'pond' < li-mal 'small pool'",
            "limer 'younger parent' < li-mer 'small parent'",
            "linai 'aunt' < li-nai 'lesser mother'",
            "lis 'child' < li-su 'little one'"
          ]
        }
      ]
