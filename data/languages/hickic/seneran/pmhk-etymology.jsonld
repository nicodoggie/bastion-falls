  },
  "baety:majorSoundChanges": [
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Final schwa dropping",
      "baety:soundChangeRule": "ə → * / _ $",
      "baety:example": "ˈhirə → ˈhir, ˈtakə → ˈtak",
      "baety:notes": "Word-final schwa [ə] was dropped, creating many consonant-final words"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Voicing of intervocalic stops",
      "baety:soundChangeRule": "[stop, -voice] → [+voice] / @vowel _ @vowel",
      "baety:example": "ˈhasəmə → ˈhasəm, ˈabəka → ˈabəga",
      "baety:notes": "Voiceless stops became voiced between vowels"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Medial h-weakening",
      "baety:soundChangeRule": "h → ʔ / @vowel _ @vowel",
      "baety:example": "ˈbahə → ˈbaʔ",
      "baety:notes": "Medial [h] weakened to glottal stop [ʔ] between vowels"
    }
  ],
