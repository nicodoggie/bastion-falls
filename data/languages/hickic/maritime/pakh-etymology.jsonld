  },
  "baety:majorSoundChanges": [
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Unstressed vowel reduction",
      "baety:soundChangeRule": "V → [+reduced] / [unstressed] _",
      "baety:example": "ˈheru → ˈhiru, ˈtaku → ˈtuku",
      "baety:notes": "Unstressed vowels were reduced: high vowels remained, low vowels became schwa (ə)"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Initial fricative vowel fronting",
      "baety:soundChangeRule": "{ə, e} → i / $ [fricative] _",
      "baety:example": "ˈseti → ˈsiti, ˈθənu → ˈθinu",
      "baety:notes": "Mid and central vowels became [i] after word-initial fricatives"
    },
    {
      "@type": "baety:RegularSoundChange",
      "dc:description": "Final vowel centralization",
      "baety:soundChangeRule": "[+high +back] → [+central], [+mid +front] → [+central] / _ $",
      "baety:example": "ˈheru → ˈhirə, ˈtaku → ˈtukə",
      "baety:notes": "Word-final high back vowels [u] and mid front vowels [e] became schwa [ə]"
    }
  ],
